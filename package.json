{
  "name": "bellotero-test-frontend",
  "version": "1.0.0",
  "description": "Frontend test for bellotero.io built with React + Next + Redux",
  "scripts": {
    "dev": "node server/index.js",
    "build": "next build",
    "start": "NODE_ENV=production node server/index.js",
    "lint": "eslint '*/*'",
    "fix-lint": "eslint --ext .js,.jsx '*/*' --fix",
    "test": "NODE_ENV=test yarn jest"
  },
  "main": "index.js",
  "repository": "git@github.com:MikeSagnelli/bellotero-test-frontend.git",
  "author": "MikeSagnelli <ing.sagnelli@gmail.com>",
  "license": "MIT",
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "dependencies": {
    "es6-promise": "^4.2.6",
    "express": "^4.16.4",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.3",
    "next-redux-saga": "^4.0.1",
    "next-redux-wrapper": "^3.0.0-alpha.1",
    "preact": "^8.4.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "styled-components": "^4.1.3",
    "styled-media-query": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-canvas-mock": "^2.0.0-alpha.3",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.3",
    "redux-devtools-extension": "^2.13.8"
